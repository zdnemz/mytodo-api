{
  "workbench.editor.customLabels.patterns": {
    "**/src/models/**/*.{ts,js}": "${filename} - Model",
    "**/src/libs/utils/**/*.{ts,js}": "${filename} - Utility",
    "**/src/libs/app/**/*.{ts,js}": "${filename} - Application",
    "**/src/controllers/**/*.{ts,js}": "${filename} - Controller",
    "**/src/middlewares/**/*.{ts,js}": "${filename} - Middleware",
    "**/src/routes/**/*.{ts,js}": "${filename} - Routes",
    "**/tests/e2e/**/*.test.{ts,js}": "${filename} - End-to-end Testing",
    "**/tests/unit/**/*.test.{ts,js}": "${filename} - Unit Testing"
  }
}
